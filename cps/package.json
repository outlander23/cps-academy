{
  "name": "cps",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo helper for frontend/backend",
  "scripts": {
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "npm install --prefix frontend",
    "install:backend": "npm install --prefix backend",
    "install-client": "npm install --prefix frontend",
    "install-server": "npm install --prefix backend",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:backend": "npm --prefix backend run dev",
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "client": "npm --prefix frontend run dev",
    "server": "npm --prefix backend run dev",
    "start:frontend": "npm --prefix frontend run preview",
    "start:backend": "npm --prefix backend start",
    "start": "concurrently \"npm:start:frontend\" \"npm:start:backend\"",
    "lint": "echo \"No linter configured. Add your linter command.\"",
    "test": "echo \"No tests configured. Add your test command.\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "express": "^5.1.0"
  }
}
